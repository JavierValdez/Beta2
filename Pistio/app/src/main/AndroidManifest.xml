        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".VentanillaActivity"></activity>
        <activity android:name=".PantallaActivity" />
        <activity android:name=".EscritorioActivity" />
        <activity android:name=".Generador" />
